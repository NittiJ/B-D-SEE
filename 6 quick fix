BDC_Export <- BDC_Cleaned_Dedup %>%
  mutate(
    PROVIDER_NAME = PROV_ORG_NM,
    NTWK = "",
    PROVTYPE = "",
    RowId = row_number(),
    Latitude = NA_character_,
    Longitude = NA_character_,
    GeoInfo = NA_character_,
    StandardZip = NA_character_,
    StandardStat = NA_character_,
    CountySSA = NA_character_
  ) %>%
  transmute(
    NTWK,
    PROVTYPE,
    NPI = PROV_NPI,
    PROVIDER_NAME,
    ADDRESS = PROV_ADDRS1,
    CITY = PROV_CITY,
    STATE = Provider_STATE,
    ZIP_5 = Provider_Zip_CODE,
    RowId,
    Latitude,
    Longitude,
    GeoInfo,
    StandardZip,
    StandardStat,
    CountySSA,
    Bdc_Program_DESCRIPTION,
    RECOGNTN_ENTITY_CD,
    Recogntn_Value_CD,
    Designation_TYPE
  )
